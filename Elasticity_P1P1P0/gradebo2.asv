function N=gradebo2(fun1,fun2,P)
x=[P(1,1),P(1,2)];
y=[P(2,1),P(2,2)];
W=[0.236926885056189 0.478628670499366 0.568888888888889 ...
    0.478628670499366 0.236926885056189];
L=[0.046910077030668 0.230765344947158 0.5 ...
    0.769234655052842 0.953089922969332];
FV=zeros(1,5);
for i=1:5
    FV(i)=fun1(x(1)+L(i)*(x(2)-x(1)),y(1)+L(i)*(y(2)-y(1)))*fun2(L(i));
end
    N=(1/2)*sum(W.*FV);
end
function y=gint_1d(fun,a,b)
weight=[0.236926885056189 0.478628670499366 0.568888888888889 ...
    0.478628670499366 0.236926885056189];
location=[0.046910077030668 0.230765344947158 0.5 ...
    0.769234655052842 0.953089922969332]';
y=0;
for i=1:5
    y=y+weight(i)*fun(a+location(i)*(b-a))*(b-a)/2;
end